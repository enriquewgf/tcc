olumns:\n","    # loop das features\n","    scaler = MinMaxScaler()\n","    decomposed_stock_features_series[stock][column] = {}\n","    #series = stock_dataframe[column].values                                          # (247,) 1D array\n","    series = stock_dataframe[column].values.reshape(-1,1)                             # (247, 1) 2D array\n","    scaler.fit(series)                                                                # Compute the minimum and maximum to be used for later scaling.\n","    scalers[stock][column] = scaler                                                   # MinMaxScaler(copy=True, feature_range=(0, 1))\n","    stock_feature_time_series = np.frombuffer(scaler.transform(series))               # scaler.transform(series) is the 2D normalized series and frombuffer makes it 1D\n","    stock_feature_time_series_imfs = ceemdan(stock_feature_time_series, max_imf=10)   # (5, 247) 247 is the number of values and 5 the number of imfs (which varies depending on the series)\n","    for i, imf_series in enumerate(stock_feature_time_series_imfs):\n","      # loop das IMF\n","      if i < len(stock_feature_time_series_imfs):                                     # If (5, 247), len is 247\n","        decomposed_stock_features_series[stock][column][f'IMF{i+1}'] = imf_series\n","      else:\n","        decomposed_stock_features_series[stock][column][f'Rsd'] = imf_series  \n","\n","# Data preprocessing\n","\n","max_window_size = 10              \n","windows_sizes_for_imf_level = {   # These are the number of previous days to predict days_ahead days\n","    'IMF1': 2,        \n","    'IMF2': 2,\n","    'IMF3': 3,\n","    'IMF4': 3,\n","    'IMF5': 4,\n","    'IMF6': 4,\n","    'IMF7': 5,\n","    'IMF8': 5,\n","    'Rsd': 6,\n","    'DEFAULT': 4\n","}\n","\n","target_feature_max_imf_level = {}\n","series = {}\n","\n","for stock in decomposed_stock_features_series:\n","  series[stock] = {}\n","  for feature in decomposed_stock_features_series[stock]:\n","    imfs = pd.DataFrame.from_dict(decomposed_stock_features_series[stock][feature])\n","    for imf in imfs:                                        # imf = 'IMF1' ou 'IMF2'\n","      if imf not in series[stock]:\n","        series[stock][imf] = []                             # {'PETR4': {'IMF1': [], 'IMF2': [], 'IMF3': [], 'IMF4': [], 'IMF5': [], 'IMF6': [], 'IMF7': []}}\n","      _series = imfs[imf].values                            # (247,)\n","      _series = _series.reshape((len(_series), 1))          # (247, 1)\n","      series[stock][imf] += [_series]                       # {'PETR4': {'IMF1': [imf_values_2D_array], 'IMF2': [], ...}} one 2D_array for each feature\n","      if feature == target_feature:\n","        target_feature_max_imf_level[stock] = imf        \n","\n","series_cut = {}\n","for stock in series:\n","  if stock not in series_cut:\n","    series_cut[stock] = {}\n","  for imf_level_string in series[stock]:\n","    imf_level_int = int(imf_level_string[3:])\n","    if imf_level_int > int(target_feature_max_imf_level[stock][3:]):\n","      continue\n","    else:\n","      series_cut[stock][imf_level_string] = series[stock][imf_level_string].copy()\n","\n","series = series_cut\n","dataset = {}\n","\n","for stock in series:\n","  dataset[stock] = {}\n","  for imf_level in series[stock]:\n","    dataset[stock][imf_level] = np.hstack( tuple(series[stock][imf_level]) )   # (247, 5)   \n","    \n","    # series[stock][imf_level] are 5 2d arrays, each of which with 247 values\n","    # tuple() makes it a tuple of arrays\n","    # hstack horizontally concatenates them, into one big array  \n","\n","train = 0.7\n","validation = 0.2\n","test = 0.1\n","\n","batch_size = 1\n","days_ahead = 7\n","\n","train_x = {}\n","train_y = {}\n","validation_x = {}\n","validation_y = {}\n","test_x = {}\n","test_y = {}\n","\n","train_generators = {}\n","validation_generators = {}\n","test_generators = {}\n","\n","for stock in dataset:\n","  train_x[stock] = {}\n","  train_y[stock] = {}\n","  validation_x[stock] = {}\n","  validation_y[stock] = {}\n","  test_x[stock] = {}\n","  test_y[stock] = {}\n","\n","  train_generators[stock] = {}\n","  validation_generators[stock] = {}\n","  test_generators[stock] = {}  \n","\n","  for imf_level in dataset[stock]:\n","    train_x[stock][imf_level], train_y[stock][imf_level] = to_multi_step(dataset[stock][imf_level][:round(train*dataset[stock][imf_level].shape[0]), :], days_ahead)\n","    validation_x[stock][imf_level], validation_y[stock][imf_level] = to_multi_step(dataset[stock][imf_level][round(train*dataset[stock][imf_level].shape[0]):round((train+validation)*dataset[stock][imf_level].shape[0]),:], days_ahead)\n","    test_x[stock][imf_level], test_y[stock][imf_level] = to_multi_step(dataset[stock][imf_level][round((train+validation)*dataset[stock][imf_level].shape[0]):,:], days_ahead)\n","\n","    if imf_level in windows_sizes_for_imf_level:\n","      window_size = windows_sizes_for_imf_level[imf_level]\n","    else:\n","      window_size = windows_sizes_for_imf_level['DEFAULT']\n","\n","    # Windowing\n","    train_generators[stock][imf_level] = TimeseriesGenerator(train_x[stock][imf_level], train_y[stock][imf_level], length=window_size, batch_size=batch_size)\n","    validation_generators[stock][imf_level] = TimeseriesGenerator(validation_x[stock][imf_level], validation_y[stock][imf_level], length=window_size, batch_size=batch_size)\n","    test_generators[stock][imf_level] = TimeseriesGenerator(test_x[stock][imf_level], test_y[stock][imf_level], length=window_size, batch_size=batch_size)\n","\n","# Took 1.5 minutes to train for 2 imfs, using a year of a single stock\n","\n","models = {}\n","\n","model_epochs = {\n","    'IMF1': 2500,\n","    'IMF2': 2000,\n","    'IMF3': 1500,\n","    'IMF4': 1500,\n","    'IMF5': 1500,\n","    'IMF6': 1200,\n","    'IMF7': 1200,\n","    'IMF8': 1000,\n","    'Rsd': 1000,\n","    'DEFAULT': 1000\n","}\n","\n","imfs_to_predict_with_neural = ['IMF1']#, 'IMF2'] # set to ['IMF1'] , ['IMF1', 'IMF2], ['IMF1','IMF2','IMF3'] and so on\n","for stock in train_generators:\n","    models[stock] = {}\n","    reached_max_imf_of_target_feature = False\n","    for imf_level in train_generators[stock]:\n","        if imf_level in imfs_to_predict_with_neural:\n","            print(f'Training model [{stock}][{imf_level}]')\n","            if reached_max_imf_of_target_feature is True:\n","                break # no need to predict further if target feature doesn't contain greater IMF levels\n","\n","            if target_feature_max_imf_level[stock] == imf_level:\n","                reached_max_imf_of_target_feature = True\n","\n","            # Prediction model\n","            model = Sequential()\n","            #current_dataset = train_dataset[stock][imf_level]\n","            n_features = train_x[stock][imf_level].shape[1]\n","            cur_tmp_gen = train_generators[stock][imf_level]\n","            cur_val_gen = validation_generators[stock][imf_level]\n","\n","            if imf_level in windows_sizes_for_imf_level:\n","                window_size = windows_sizes_for_imf_level[imf_level]\n","            else: \n","                window_size = windows_sizes_for_imf_level['DEFAULT']\n","\n","            #model.add(LSTM(128, activation='tanh', return_sequences=True, input_shape=(window_size, n_features)))\n","            #model.add(LSTM(64, activation='tanh', input_shape=(window_size, 128)))\n","            model.add(CuDNNLSTM(128, return_sequences=True, input_shape=(window_size, n_features), stateful=True))\n","            model.add(Activation(tanh))\n","            model.add(CuDNNLSTM(64, input_shape=(window_size, 128)))\n","            model.add(Activation(tanh))\n","            model.add(Dense(16))\n","            model.add(LeakyReLU())\n","            model.add(Dense(4))\n","            model.add(LeakyReLU())\n","            model.add(Dense(days_ahead)) # Original was 1, don't know if going from 16 to 4 and then to 7, 30, 120, 365 is the best ideia.\n","            model.compile(optimizer='adam', loss='mse')\n","\n","            number_of_epochs = model_epochs[imf_level]\n","\n","            # fit model\n","            model.fit(cur_tmp_gen, steps_per_epoch=1, epochs=number_of_epochs, verbose=1)\n","            models[stock][imf_level] = model\n","\n","        else:\n","            # Spline prediction model\n","            cur_tmp_gen = train_generators[stock][imf_level]\n","            model = SplineModel(cur_tmp_gen)\n","            models[stock][imf_level] = model\n","\n","full_results = {}\n","\n","for stock in models:\n","    full_results[stock] = {}\n","\n","    # initializing results dicitionary\n","    for feature in features_in_order:         \n","        if feature != target_feature:\n","          continue       \n","\n","        full_results[stock][feature] = {}\n","\n","        for imf_level in models[stock]:\n","            full_results[stock][feature][imf_level] = {'x_axis': []}\n","            cur_train_gen = train_generators[stock][imf_level]\n","            cur_validation_gen = validation_generators[stock][imf_level]\n","            cur_test_gen = test_generators[stock][imf_level]\n","            for j in range(len(cur_train_gen) + len(cur_validation_gen) + len(cur_test_gen) + days_ahead):\n","              full_results[stock][feature][imf_level]['x_axis'].append(j)\n","              full_results[stock][feature][imf_level]['x_axis'][j] = {\n","                  'real_train': [],\n","                  'predicted_train': [],\n","                  'single_predicted_train': [],\n","                  'real_validation': [],\n","                  'predicted_validation': [],\n","                  'single_predicted_validation': [],\n","                  'real_test': [],\n","                  'predicted_test': [],\n","                  'single_predicted_test': []\n","              }\n","\n","    for imf_level in models[stock]:\n","      model = models[stock][imf_level]\n","\n","      print(f'Predicting: [{stock}][{imf_level}]')\n","\n","      cur_train_gen = train_generators[stock][imf_level]\n","      cur_validation_gen = validation_generators[stock][imf_level]\n","      cur_test_gen = test_generators[stock][imf_level]\n","\n","      # predicting train\n","      day_counter = 0\n","      for i in range(len(cur_train_gen)):\n","          x, y = cur_train_gen[i]\n","          if model.name == 'SplineModel': yhat = model.predict(x, days_ahead, verbose=0)\n","          else: yhat = model.predict(x, verbose=0)\n","\n","          full_results[stock][target_feature][imf_level]['x_axis'][day_counter]['real_train'] += [y[0][0]]\n","                    \n","          for j in range(yhat.shape[1]):\n","            full_results[stock][target_feature][imf_level]['x_axis'][day_counter + j]['predicted_train'] += [yhat[0][j]]\n","\n","          day_counter += 1\n","\n","      first_validation_day = day_counter\n","\n","      # Calculating the average of all predictions for each day\n","      for j in range(len(cur_train_gen)):\n","        acc = 0\n","        num_preds = len(full_results[stock][target_feature][imf_level]['x_axis'][j]['predicted_train'])\n","        for i in range(num_preds):    \n","          acc += full_results[stock][target_feature][imf_level]['x_axis'][j]['predicted_train'][i]\n","\n","        full_results[stock][target_feature][imf_level]['x_axis'][j]['single_predicted_train'] += [acc / num_preds]\n","\n","\n","      # predicting validation\n","      for i in range(len(cur_validation_gen)):\n","          x, y = cur_validation_gen[i]\n","          if model.name == 'SplineModel': yhat = model.predict(x, days_ahead, verbose=0)\n","          else: yhat = model.predict(x, verbose=0)\n","\n","          full_results[stock][target_feature][imf_level]['x_axis'][day_counter]['real_validation'] += [y[0][0]]\n","                    \n","          for j in range(yhat.shape[1]):\n","            full_results[stock][target_feature][imf_level]['x_axis'][day_counter + j]['predicted_validation'] += [yhat[0][j]]\n","\n","          day_counter += 1\n","\n","      first_test_day = day_counter\n","\n","      # Calculating the average of all predictions for each day\n","      for j in range(len(cur_validation_gen)): \n","        acc = 0\n","        num_preds = len(full_results[stock][target_feature][imf_level]['x_axis'][first_validation_day + j]['predicted_validation'])\n","        for i in range(num_preds):    \n","          acc += full_results[stock][target_feature][imf_level]['x_axis'][first_validation_day + j]['predicted_validation'][i]\n","\n","        full_results[stock][target_feature][imf_level]['x_axis'][first_validation_day + j]['single_predicted_validation'] += [acc / num_preds]\n","\n","\n","      # predicting test\n","      for i in range(len(cur_test_gen) - 1):\n","          x, y = cur_test_gen[i]\n","          if model.name == 'SplineModel': yhat = model.predict(x, days_ahead, verbose=0)\n","          else: yhat = model.predict(x, verbose=0)\n","\n","          full_results[stock][target_feature][imf_level]['x_axis'][day_counter]['real_test'] += [y[0][0]]\n","                    \n","          for j in range(yhat.shape[1]):\n","            try:\n","              full_results[stock][target_feature][imf_level]['x_axis'][day_counter + j]['predicted_test'] += [yhat[0][j]]\n","            except:\n","              print(f'{day_counter + j}, {imf_level}')\n","\n","          day_counter += 1\n","\n","      # Calculating the average of all predictions for each day\n","      for j in range(len(cur_test_gen) - 1): # The last day has no predictions\n","        acc = 0\n","        num_preds = len(full_results[stock][target_feature][imf_level]['x_axis'][first_test_day + j]['predicted_test'])\n","        for i in range(num_preds):    \n","          acc += full_results[stock][target_feature][imf_level]['x_axis'][first_test_day + j]['predicted_test'][i]\n","\n","        full_results[stock][target_feature][imf_level]['x_axis'][first_test_day + j]['single_predicted_test'] += [acc / num_preds]\n","\n","# Copying full results to original results format\n","\n","results = {}\n","\n","for stock in models:\n","    results[stock] = {}\n","\n","    # initializing results dicitionary\n","    for feature in features_in_order:         \n","        if feature != target_feature:\n","          continue       \n","\n","        results[stock][feature] = {}\n","\n","        for imf_level in models[stock]:\n","            results[stock][feature][imf_level] = {\n","                'real_train': [],\n","                'predicted_train': [],\n","                'x_axis_train': [],\n","                'real_validation': [],\n","                'predicted_validation': [],\n","                'x_axis_validation': [],\n","                'real_test': [],\n","                'predicted_test': [],\n","                'x_axis_test': []\n","            }\n","\n","    for imf_level in models[stock]:\n","        model = models[stock][imf_level]\n","        \n","        print(f'Predicting: [{stock}][{imf_level}]')\n","\n","        cur_train_gen = train_generators[stock][imf_level]\n","        cur_validation_gen = validation_generators[stock][imf_level]\n","        cur_test_gen = test_generators[stock][imf_level]\n","\n","        # predicting train\n","        day_counter = 0\n","        for i in range(len(cur_train_gen)):\n","            results[stock][target_feature][imf_level]['real_train'] += [full_results[stock][target_feature][imf_level]['x_axis'][day_counter]['real_train'][0]]\n","            results[stock][target_feature][imf_level]['predicted_train'] += [full_results[stock][target_feature][imf_level]['x_axis'][day_counter]['single_predicted_train'][0]]\n","            results[stock][target_feature][imf_level]['x_axis_train'] += [day_counter]\n","            day_counter += 1\n","\n","\n","        # predicting validation\n","        for i in range(len(cur_validation_gen)):\n","            results[stock][target_featur